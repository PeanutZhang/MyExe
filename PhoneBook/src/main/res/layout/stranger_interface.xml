<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <TextView
    android:id="@+id/strange_quit"
    android:layout_width="320dp"
    android:layout_height="wrap_content"
    android:background="#696969"
    android:drawableLeft="@drawable/y5"
    android:drawablePadding="25dp"
    android:gravity="center_vertical"
    android:text="@string/sstranger_set"
    android:textColor="@color/blak"
    android:textSize="20sp"
    android:onClick="quit"
     />
    
    <LinearLayout 
    android:orientation="vertical" 
    android:background="@color/wwhite"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    
        >
     <TextView 
     android:id="@+id/strange_smsinstall"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:text="@string/smsinstall"
     android:textSize="16sp"
     android:textColor="@color/blak"
     android:paddingTop="20dp"
     android:paddingBottom="10dp"
     />
  <View 
      android:layout_width="fill_parent"
      android:layout_height="1dip"
      android:background="@color/gray"
      />
  <TextView 
      android:id="@+id/strange_timeinstall"
      android:layout_width="fill_parent"
      android:layout_height="wrap_content"
      android:text="@string/timeinstrall"
      android:textSize="16sp"
      android:textColor="@color/blak"
        android:paddingTop="10dp"
     android:paddingBottom="12dp"
      />
  <TextView 
      android:layout_width="fill_parent"
      android:layout_height="wrap_content"
      android:text="@string/mast"
      android:textSize="21sp"
      android:textColor="@color/yell"
      android:paddingLeft="10dip"
        android:paddingTop="5dp"
     android:paddingBottom="7dp"
      />
  <View 
      android:layout_width="fill_parent"
      android:layout_height="2dip"
      android:background="@color/yell"
      />
<LinearLayout 
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:id="@+id/strange_smslinear"
    android:gravity="center_vertical"
       android:paddingTop="15dp"
     android:paddingBottom="10dp"
    >
     <TextView 
     android:layout_width="200sp"
     android:layout_height="50sp"
     android:text="@string/smsfrequency"
     android:textSize="16sp"
     android:textColor="@color/blak"
     android:paddingRight="70sp"
     />
     <TextView 
         android:layout_width="match_parent"
         android:layout_height="50sp"
         android:id="@+id/strange_smssinstall"
         android:paddingTop="23dp"
         />
</LinearLayout>
  <View 
      android:layout_width="fill_parent"
      android:layout_height="1dip"
      android:background="@color/gray"
      />
<LinearLayout 
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:id="@+id/strange_phonelinear"
    android:gravity="center_vertical"
      android:paddingTop="10dp"
     android:paddingBottom="10dp"
    >
     <TextView 
     android:layout_width="200sp"
     android:layout_height="50sp"
     android:text="@string/phonefrequency"
     android:textSize="16sp"
     android:textColor="@color/blak"
     android:paddingRight="50dp"
     />
     <TextView 
         android:layout_width="match_parent"
         android:layout_height="50sp"
         android:id="@+id/strange_phoneinstall"
         android:paddingTop="23dp"
       />
</LinearLayout>
  <View 
      android:layout_width="fill_parent"
      android:layout_height="1dip"
      android:background="@color/gray"
      />
<LinearLayout 
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:id="@+id/strange_timelinear"
    android:gravity="center_vertical"
    android:paddingRight="70sp"
       android:paddingTop="10dp"
     android:paddingBottom="1dp"
    >
     <TextView 
     android:layout_width="200sp"
     android:layout_height="50sp"
     android:text="@string/timespan"
     android:textSize="16sp"
     android:textColor="@color/blak"
     />
     <TextView 
         android:layout_width="match_parent"
         android:layout_height="50sp"
         android:id="@+id/strange_timespaneinstall"
       />
</LinearLayout>
 <View 
      android:layout_width="fill_parent"
      android:layout_height="1dip"
      android:background="@color/gray"
      />
    </LinearLayout>

</LinearLayout>
